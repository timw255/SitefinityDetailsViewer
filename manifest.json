{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "style.css", "jquery.minicolors.css" ],
      "js": [ "jquery-1.9.1.min.js", "contentscript.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Displays details about Sitefinity CMS websites and adds features to the back-end page editor",
   "icons": {
      "128": "sf-128.png",
      "16": "sf-16.png",
      "48": "sf-48.png"
   },
   "manifest_version": 2,
   "name": "Sitefinity CMS Detail Viewer",
   "options_page": "options.html",
   "page_action": {
      "default_popup": "popup.html",
      "default_title": "Show details"
   },
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.5.5",
   "web_accessible_resources": [ "collapse_icon.png", "expand_icon.png", "ajax-loader.gif", "style.css" ]
}
